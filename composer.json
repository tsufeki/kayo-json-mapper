{
    "name": "tsufeki/kayo-json-mapper",
    "description": "Map JSON data to PHP objects",
    "license": "MIT",
    "authors": [
        {
            "name": "tsufeki",
            "email": "tsufeki@ymail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "phpdocumentor/type-resolver": "^1.0.1",
        "phpdocumentor/reflection-docblock": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-phpunit": "^0.11"
    },
    "autoload": {
        "psr-4": {
            "Tsufeki\\KayoJsonMapper\\": "src/Tsufeki/KayoJsonMapper/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Tsufeki\\KayoJsonMapper\\": "tests/Tsufeki/KayoJsonMapper/"
        }
    }
}
